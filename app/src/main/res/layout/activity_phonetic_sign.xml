<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".phometic.PhoneticSignActivity">

    <com.qmuiteam.qmui.widget.QMUITopBarLayout
        android:id="@+id/phonetic_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        android:background="@color/qmui_config_color_50_blue"></com.qmuiteam.qmui.widget.QMUITopBarLayout>

    <FrameLayout
        android:id="@+id/phonetic_sign_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:layout_marginTop="?attr/qmui_topbar_height">

    </FrameLayout>

</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>